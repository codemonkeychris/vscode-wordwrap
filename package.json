{
	"name": "WordWrap",
	"description": "Word wrap at a specific column",
	"version": "0.0.1",
	"publisher": "Chris Anderson",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.wordWrap"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.wordWrap",
			"title": "Word Wrap"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}